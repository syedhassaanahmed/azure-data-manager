								"type": "text/javascript",
								"exec": [
									"if(pm.globals.get(\"bearerToken\") !== \"\") {",
									"    pm.sendRequest({",
									"        url: \"https://login.microsoftonline.com/\" + pm.environment.get(\"tenantId\") + \"/oauth2/token\",",
									"        method: \"POST\",",
									"        header: \"Content-Type: application/x-www-form-urlencoded\",",
									"        body: {",
									"            mode: \"urlencoded\",",
									"            urlencoded: [ ",
									"                {key: \"grant_type\", value: \"authorization_code\", disabled: false},",
									"                {key: \"client_id\", value: pm.environment.get(\"clientId\"), disabled: false},",
									"                {key: \"client_secret\", value: encodeURIComponent(pm.environment.get(\"clientSecret\")), disabled: false},",
									"                {key: \"auth_url\", value: \"https://login.microsoftonline.com/\" + pm.environment.get(\"tenantId\") + \"/oauth2/authorize?resource=\" + pm.environment.get(\"clientId\"), disabled: false},",
									"                {key: \"redirect_uri\", value: pm.environment.get(\"webAppUrl\"), disabled: false}",
									"            ]",
									"        }",
									"    }, function (err, res) {",
									"        pm.globals.set(\"bearerToken\", res.json().access_token);",
									"        console.log(pm.globals.get(\"bearerToken\"));",
									"    });",
									"}"
								]
							}
						}
					],
